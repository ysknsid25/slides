{
  "name": "slides",
  "private": true,
"scripts": {
    "new-talk": "node scripts/new-talk.mjs",
    "prebuild": "node scripts/prebuild.mjs",
    "build": "node scripts/prebuild.mjs && pnpm --filter './talks/**' run build && pnpm --filter './talks/**' run export:og",
    "build:one": "node scripts/prebuild.mjs && pnpm --filter",
    "gen-index": "node scripts/gen-index.mjs",
    "export:og": "pnpm --filter './talks/**' run export:og",
    "dev": "pnpm --filter './talks/**' run dev",
    "deploy": "pnpm build && firebase deploy",
    "deploy:incremental": "pnpm gen-index && firebase deploy"
  }
}
